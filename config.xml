<?xml version='1.1' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Health App</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <plugin name="cordova-plugin-splashscreen" source="npm" spec="5.0.2"  />
        <platform name="android">
	    <!-- you can use any density that exists in the Android project -->

	    <splash src="res/drawable-hdpi/screen_ldpi_portrait.9.png" density="port-hdpi"/>
	    <splash src="res/drawable-xxdpi/screen_ldpi_portrait.9.png" density="port-xxhdpi"/>
	    <splash src="res/drawable-xxxhdpi/screen_ldpi_portrait.9.png" density="port-xxxhdpi"/>
	    <splash src="res/drawable-mdpi/screen_ldpi_portrait.9.png" density="port-mdpi"/>
    	    <splash src="res/drawable-xhdpi/screen_ldpi_portrait.9.png" density="port-xhdpi"/>
	</platform>
        <preference name="SplashScreenDelay" value="5000" />
	

    <content src="index.html" />
    <access origin="*" />
</widget>
